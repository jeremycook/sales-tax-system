@model IEnumerable<Cohub.Data.Fin.PaymentChart>
@inject IStringLocalizer<Cohub.Data.Fin.PaymentChart> L
@{ ViewBag._Title = L["Payment Charts"]; }
<div class="pad">
    <h1>@ViewBag._Title</h1>

    <table class="card">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(m => m.Id)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.CategoryId)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.FrequencyId)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.Created)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        <a href="@Url.Action("Details", new { id = item.Id })">@item.Id</a>
                    </td>
                    <td>
                        @Html.DisplayFor(_ => item.CategoryId)
                    </td>
                    <td>
                        @Html.DisplayFor(_ => item.FrequencyId)
                    </td>
                    <td class="text-center">
                        @Html.DisplayFor(_ => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(_ => item.Created)
                    </td>
                </tr>
            }
        </tbody>
    </table>

</div>
