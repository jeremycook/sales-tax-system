@model Report
@inject IStringLocalizer<Report> L
@{
    ViewBag._Title = L["Delete Report"];
}
<div class="pad">
    <h1>@ViewBag._Title</h1>

    <form method="post">
        <partial name="UI/ValidationSummary" />

        <p>
            @SL["Delete {0}?", Model]
        </p>

        <div class="input-group">
            <button class="button">@SL["Yes"]</button>
            <a class="button" href="@Url.Action("Index")">@SL["No"]</a>
        </div>
    </form>
</div>
